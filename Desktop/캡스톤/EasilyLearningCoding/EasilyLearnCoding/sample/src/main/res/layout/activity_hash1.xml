<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WebView.hash1">

    <WebView
        android:id="@+id/WebView_hash1"
        android:layout_width="match_parent"
        android:layout_height="663dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/hashcode"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <Button
            android:id="@+id/bracel"
            style="@android:style/ButtonBar"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginLeft="2dp"
            android:gravity="center"
            android:text="{"
            android:textSize="40dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/quot"
            style="@android:style/ButtonBar"
            android:layout_width="65dp"
            android:layout_height="65dp"

            android:layout_marginLeft="3dp"
            android:gravity="center"
            android:text="&quot;"
            android:textSize="40dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/bracel" />
        <Button
            android:id="@+id/equal"
            style="@android:style/ButtonBar"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginLeft="3dp"
            android:gravity="center"
            android:text="="
            android:textSize="40dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/quot" />
        <Button
            android:id="@+id/Semicolon"
            style="@android:style/ButtonBar"
            android:layout_marginLeft="3dp"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:gravity="center"
            android:text=";"
            android:textSize="40dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/slash" />
        <Button
            android:id="@+id/slash"
            style="@android:style/ButtonBar"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginLeft="3dp"
            android:gravity="center"
            android:text="&amp;"
            android:textSize="40dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/equal" />
        <Button
            android:id="@+id/bracer"
            style="@android:style/ButtonBar"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginLeft="3dp"
            android:gravity="center"
            android:text="}"
            android:textSize="40dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/Semicolon" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/hide"
        style="@android:style/ButtonBar"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_marginBottom="70dp"
        android:layout_marginRight="3dp"
        android:gravity="center"
        android:text="OFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/hashcode"
        app:layout_constraintEnd_toEndOf="@+id/WebView_hash1" />

</androidx.constraintlayout.widget.ConstraintLayout>